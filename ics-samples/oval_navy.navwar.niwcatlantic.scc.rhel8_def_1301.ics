DEF
CRI AND
CTN textfilecontent54
    TEST `only one exists` all
    STATE ste_1301
        subexpression string equals `^-oMACs=hmac-sha2-512-etm@openssh\.com,hmac-sha2-256-etm@openssh\.com,hmac-sha2-512,hmac-sha2-256$`
    STATE_END
    OBJECT obj_1301
        filepath `/etc/crypto-policies/back-ends/opensshserver.config`
        pattern `^CRYPTO_POLICY=[^#]*(-oMACs=.*?)\s+`
        instance `1`
        behavior multiline true
    OBJECT_END
CTN_END
CRI_END
DEF_END