DEF
STATE ste_124201
    security_principle string equals `Administrators`
STATE_END
STATE ste_124204
    security_principle string equals `SERVICE`
STATE_END
STATE ste_124202
    security_principle string equals `LOCAL SERVICE`
STATE_END
STATE ste_124203
    security_principle string equals `NETWORK SERVICE`
STATE_END
SET obj_124200 union
    OBJECT obj_124201
        security_principle `.*`
    OBJECT_END
    FILTER include
        STATE_REF oval:mil.disa.stig.windows:ste:124201
    FILTER_END
    FILTER include
        STATE_REF oval:mil.disa.stig.windows:ste:124202
    FILTER_END
    FILTER include
        STATE_REF oval:mil.disa.stig.windows:ste:124203
    FILTER_END
    FILTER include
        STATE_REF oval:mil.disa.stig.windows:ste:124204
    FILTER_END
SET_END
CRI AND
CTN accesstoken
    TEST `any exist` all
    STATE ste_124200
        secreateglobalprivilege boolean equals `0`
    STATE_END
    OBJECT obj_124200
        SET_REF obj_124200
    OBJECT_END
CTN_END
CRI_END
DEF_END