DEF
STATE ste_2901
    text string equals `^\S+\s+\S+\s+\S+\s+(\S*,)?logging(,\S*)?\s+`
STATE_END
STATE ste_2902
    text string equals `^\S+\s+\S+\s+(vxfs|zfs|hsfs)\s+`
STATE_END
STATE ste_2900
    text string equals `^/dev/fd.*$`
STATE_END
SET obj_2901 union
    OBJECT obj_2900
        filepath `/etc/mnttab`
        pattern `^/dev/.*$`
        instance `1`
    OBJECT_END
    FILTER include
        STATE_REF oval:mil.disa.stig.solaris11:ste:2900
    FILTER_END
    FILTER include
        STATE_REF oval:mil.disa.stig.solaris11:ste:2901
    FILTER_END
    FILTER include
        STATE_REF oval:mil.disa.stig.solaris11:ste:2902
    FILTER_END
SET_END
CRI AND
CTN textfilecontent54
    TEST `none exist` all
    OBJECT obj_2901
        SET_REF obj_2901
    OBJECT_END
CTN_END
CRI_END
DEF_END