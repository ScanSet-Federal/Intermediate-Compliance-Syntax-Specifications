    STATE ste_20000001
        type string equals `reg_dword`
        value int equals `1`
    STATE_END
    OBJECT obj_25335200
        hive `HKEY_LOCAL_MACHINE`
        key `SOFTWARE\Policies\Microsoft\Windows\Personalization`
        name `NoLockScreenSlideshow`
    OBJECT_END
CTN_END
CRI_END
