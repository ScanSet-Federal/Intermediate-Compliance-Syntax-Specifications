DEF
CRI AND
CTN textfilecontent54
    TEST `any exist` all OR
    STATE ste_23039200
        subexpression string equals `(?i)^syslog$`
    STATE_END
    STATE ste_23039201
        subexpression string equals `(?i)^single$`
    STATE_END
    STATE ste_23039202
        subexpression string equals `(?i)^halt$`
    STATE_END
    OBJECT obj_23039200
        filepath `/etc/audit/auditd.conf`
        pattern `(?i)^\s*disk_full_action\s*=\s*(\w+)\s*(?:#.*)?$`
        instance `1`
    OBJECT_END
CTN_END
CRI_END
DEF_END