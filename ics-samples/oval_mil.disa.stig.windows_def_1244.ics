DEF
STATE ste_124400
    secreatesymboliclinkprivilege boolean equals `0`
STATE_END
STATE ste_124402
    security_principle string equals `NT VIRTUAL MACHINE\Virtual Machines`
STATE_END
STATE ste_124401
    security_principle string equals `Administrators`
STATE_END
SET obj_124400 union
    OBJECT obj_124402
        security_principle `.*`
    OBJECT_END
    FILTER include
        STATE_REF oval:mil.disa.stig.windows:ste:124401
    FILTER_END
SET_END
SET obj_124401 union
    OBJECT obj_124402
        security_principle `.*`
    OBJECT_END
    FILTER include
        STATE_REF oval:mil.disa.stig.windows:ste:124401
    FILTER_END
    FILTER include
        STATE_REF oval:mil.disa.stig.windows:ste:124402
    FILTER_END
SET_END
CRI OR
CTN accesstoken
    TEST `any exist` all
    STATE_REF ste_124400
    OBJECT obj_124400
        SET_REF obj_124400
    OBJECT_END
CTN_END
CRI AND
CTN accesstoken
    TEST `any exist` all
    STATE_REF ste_124400
    OBJECT obj_124401
        SET_REF obj_124401
    OBJECT_END
CTN_END
CTN wmi
    TEST `at least one exists` all
    STATE ste_124403
        result string equals `Hyper-V` `all`
    STATE_END
    OBJECT obj_124403
        namespace `root\cimv2`
        wql `SELECT name FROM Win32_ServerFeature WHERE ID = 20`
    OBJECT_END
CTN_END
CRI_END
CRI_END
DEF_END