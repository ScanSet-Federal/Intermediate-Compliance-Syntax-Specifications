DEF
CRI AND
CTN textfilecontent54
    TEST `any exist` all OR
    STATE ste_23039400
        subexpression string equals `(?i)^hostname$`
    STATE_END
    STATE ste_23039401
        subexpression string equals `(?i)^fqd$`
    STATE_END
    STATE ste_23039402
        subexpression string equals `(?i)^numeric$`
    STATE_END
    OBJECT obj_23039400
        filepath `/etc/audit/auditd.conf`
        pattern `(?i)^\s*name_format\s*=\s*(\w+)\s*(?:#.*)?$`
        instance `1`
    OBJECT_END
CTN_END
CRI_END
DEF_END