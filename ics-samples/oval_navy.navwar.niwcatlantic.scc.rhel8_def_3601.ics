DEF
VAR var_3601 string ``
RUN var_3601 EXTRACT
    OBJ obj_3602 stdout_line
RUN_END
STATE ste_3602
    filename string equals `^\.`
STATE_END
STATE ste_3601
    gexec boolean equals `0`
    oread boolean equals `0`
    owrite boolean equals `0`
    oexec boolean equals `0`
STATE_END
CRI AND
CTN file
    TEST `none exist` all
    OBJECT obj_3601
        path VAR var_3601
        filename `.*`
        behavior recurse directories recurse_file_system local max_depth -1
        FILTER include
            STATE_REF ste_3601
        FILTER_END
        FILTER include
            STATE_REF ste_3602
        FILTER_END
    OBJECT_END
CTN_END
CTN shellcommand
    TEST `any exist` all
    OBJECT obj_3602
        shell `bash`
        command `ls -d $(awk -F: '($3>=1000)&&($7 !~ /nologin/){print $6}' /etc/passwd)`
    OBJECT_END
CTN_END
CRI_END
DEF_END